{
  "name": "@krain/ui",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./globals.css": "./src/globals.css",
    "./postcss.config.mjs": "./postcss.config.mjs",
    "./tailwind.config": "./tailwind.config.ts",
    "./lib/*": "./src/lib/*.ts",
    "./hooks/*": [
      "./src/hooks/*.ts",
      "./src/hooks/*.tsx"
    ],
    "./components/*": "./src/components/*.tsx",
    "./components/providers/*": "./src/components/providers/*.tsx",
    "./layouts/*": "./src/layouts/*.tsx"
  },
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "lint": "eslint .",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@intercom/messenger-js-sdk": "^0.0.14",
    "@krain/session": "workspace:*",
    "@krain/utils": "workspace:",
    "@privy-io/react-auth": "2.6.2",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.5",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@solana/web3.js": "^1.95.8",
    "@vercel/analytics": "^1.4.1",
    "@vercel/speed-insights": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^1.0.4",
    "embla-carousel-react": "^8.5.2",
    "lucide-react": "^0.479.0",
    "next-themes": "^0.4.5",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-hook-form": "^7.54.1",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.12",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@krain/eslint-config": "workspace:",
    "@krain/typescript-config": "workspace:",
    "@tailwindcss/postcss": "^4.0.12",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "eslint": "^9.22.0",
    "typescript": "^5.8.2"
  }
}
