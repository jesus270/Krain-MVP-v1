{
  "name": "@krain/session",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.mjs",
      "require": "./dist/server.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist .turbo node_modules",
    "lint": "eslint . --max-warnings 0"
  },
  "dependencies": {
    "@krain/db": "workspace:*",
    "@krain/utils": "workspace:*",
    "@privy-io/react-auth": "^2.6.2",
    "react": "catalog:",
    "@privy-io/server-auth": "^1.19.0",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.28.3",
    "iron-session": "^8.0.1",
    "next": "catalog:",
    "server-only": "^0.0.1",
    "zod": "^3.22.4",
    "@krain/typescript-config": "workspace:*"
  },
  "devDependencies": {
    "@krain/eslint-config": "workspace:*",
    "@types/node": "^20.12.12",
    "@types/react": "catalog:default",
    "@types/react-dom": "catalog:default",
    "eslint": "^9.22.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
}
